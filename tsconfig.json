{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "strict": true,
    "strictNullChecks": true
  },
  "lib": ["es2016"]
}
